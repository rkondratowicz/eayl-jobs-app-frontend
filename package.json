{
  "name": "eayl-jobs-app-frontend",
  "version": "1.0.0",
  "description": "A modern Node.js TypeScript application with ES modules",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "serve": "npm run build && npm start",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome ci ."
  },
  "keywords": [
    "typescript",
    "node",
    "es-modules",
    "tsx"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/express": "^5.0.3",
    "@types/node": "^20.8.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
